<div style="height: 70px"> </div>

<h3 class="post_wrapper"> My Jobs </h3> 
  <div class="post_wrapper" style="background-color: #f5f5f5">
  

<table style="width: 950px; background-color: #f5f5f5; height:10%" > 

 <thead style="background-color: #f5f5f5">
    <tr>
 
      <th>Title</th>
      <th>Deadline</th>
      <th>Description</th>
      <th>Text</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  </div>

  <tbody>
    <% @jobs.each do |job| %>
      <tr style="border: 5px solid #f5f5f5" >
        <td><%= link_to job.title, job %></td>
        <td><%= job.deadline%></td>
        <td><%= job.description %></td>
        <td><button> <%= link_to 'All Applications', job %> </button></td>
        <td><%= link_to 'Edit', edit_job_path(job) %></td>
        <td><%= link_to 'Destroy', job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  
      </tr>
    <% end %>
  </tbody>
</table>
</div> 

<div class="post-wrapper"> <%= link_to "Post New Job", new_job_path %> </div>
